{
  "index_patterns": [
    "zeebe-record_user_*"
  ],
  "composed_of": [
    "zeebe-record"
  ],
  "priority": 20,
  "version": 1,
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 0,
      "index.queries.cache.enabled": false
    },
    "aliases": {
      "zeebe-record-user": {}
    },
    "mappings": {
      "properties": {
        "value": {
          "dynamic": "strict",
          "properties": {
            "userKey": {
              "type": "long"
            },
            "username": {
              "type": "keyword"
            },
            "name": {
              "type": "keyword"
            },
            "email": {
              "type": "keyword"
            },
            "password": {
              "type": "keyword"
            },
            "userType": {
              "type": "keyword"
            }
          }
        }
      }
    }
  }
}
